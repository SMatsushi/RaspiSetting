[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
	worktree = /
	editor = vi
[alias]
	st=status
	br=branch
	ci=checkin
	lola = log --graph --decorate --pretty=oneline --abbrev=commit --all
[remote "origin"]
	url = https://github.com/SMatsushi/RaspiSetting.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
